    "devDependencies": {
        "@types/json-bigint": "^1.0.4",
        "@typescript-eslint/eslint-plugin": "^3.10.1",
        "@typescript-eslint/parser": "^3.10.1",
        "eslint": "^7.32.0",
        "eslint-config-prettier": "^6.15.0",
        "eslint-plugin-prettier": "^3.4.1",
        "prettier": "^2.8.8",
        "rewire": "^3.0.2",
        "typescript": "^3.9.10"
